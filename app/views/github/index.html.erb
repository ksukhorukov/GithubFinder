<div class="conteiner-fluid">
	<div class="col logo">
		<h1>Github Finder</h1>
	</div>
	<div class="col search-results">
		<center>
			<h2>Search results</h2>
		</center>
		<br/>
		<%= render partial: 'search', locals: { results: @results } %>
	</div>
	<div class="col navigation">
		<%= raw pagination(@results, @repo_name, @per_page, @page) %>
	</div>
</div>
